{
  "env": {
    "node": true
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "curly": 2,
    "indent": [2, 2],
    "max-len": [1, 80, 2]
    "no-array-constructor": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-semi": 1,
    "no-inner-declarations": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single", "avoid-escape"]
    "semi": 2,
  }
}
